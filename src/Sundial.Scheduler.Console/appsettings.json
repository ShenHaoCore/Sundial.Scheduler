{
  "App": {
    "ServiceName": "SUNDIAL SCHEDULER SERVICE",
    "DisplayName": "SUNDIAL SERVICE",
    "Description": "日冕任务调度服务"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Information",
        "Microsoft": "Information",
        "Topshelf": "Information",
        "Quartz": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.log",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "Quartz": {
    "Jobs": [
      {
        "Name": "TestJob",
        "Group": "DailyJob",
        "CronExpression": "0/10 * * * * ?",
        "JobType": "Sundial.Scheduler.Jobs.TestJob"
      },
      {
        "Name": "BilibiliJob",
        "Group": "DailyJob",
        "CronExpression": "0/10 * * * * ?",
        "JobType": "Sundial.Scheduler.Jobs.BilibiliJob"
      }
    ]
  }
}